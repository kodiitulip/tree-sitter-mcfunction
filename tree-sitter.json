  "grammars": [
    {
      "name": "mcfunction",
      "camelcase": "Mcfunction",
      "title": "Mcfunction",
      "scope": "source.mcfunction",
      "file-types": ["mcfunction"],
      "injection-regex": "^mcfunction$",
      "class-name": "TreeSitterMcfunction"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Mcfunction grammar for tree-sitter",
    "authors": [
      {
        "name": "kodiitulip",
        "email": "kodii.tulip@gmail.com",
        "url": "https://github.com/kodiitulip"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-mcfunction"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
