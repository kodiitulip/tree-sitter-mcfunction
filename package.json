{
  "name": "tree-sitter-mcfunction",
  "version": "0.1.0",
  "description": "Mcfunction grammar for tree-sitter",
  "repository": "https://github.com/tree-sitter/tree-sitter-mcfunction",
  "license": "MIT",
  "author": {
    "name": "kodiitulip",
    "email": "kodii.tulip@gmail.com",
    "url": "https://github.com/kodiitulip"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "mcfunction"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.3.1",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.25.8"
  },
  "peerDependencies": {
    "tree-sitter": "^0.22.4"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js",
    "b": "tree-sitter generate && node-gyp build",
    "t": "tree-sitter test",
    "bt": "bun b && bun t"
  }
}
